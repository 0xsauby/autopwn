# autopwn yaml tool config file
# 201502 v0.1

# The name as it will appear in the autopwn menu
name: test

# Location of the file on filesystem
binary_location: /usr/bin/test

# Menu where tool will exist
# If the menu doesn't exist it will be created
menu: lel

# Tool arguments
# Python's str.format() is used for variable replacement. Valid entries are:
# * target_domain_name
# * target_ip
# * date (current date in format 20150226_213959) # date =  strftime("%Y%m%d_%H%M%S")
# * port_number (if target port number has been specified)
arguments: -A -sS -sC -sV -p1-65535 {target_domain_name} -oA {date}_test_{target_domain_name}_full_range
